; Umbrella, inc. Cursor set made by Turion
; 2012 February 22nd

[Version]
signature="$CHICAGO$"

[DefaultInstall]
CopyFiles = Scheme.Cur, Scheme.Txt
AddReg    = Scheme.Reg

[DestinationDirs]
Scheme.Cur = 10,"%CUR_DIR%"
Scheme.Txt = 10,"%CUR_DIR%"

[Scheme.Reg]
HKCU,"Control Panel\Cursors\Schemes","%SCHEME_NAME%",,"%10%\%CUR_DIR%\%pointer%,%10%\%CUR_DIR%\%help%,%10%\%CUR_DIR%\%work%,%10%\%CUR_DIR%\%busy%,%10%\%CUR_DIR%\%cross%,%10%\%CUR_DIR%\%Text%,%10%\%CUR_DIR%\%Hand%,%10%\%CUR_DIR%\%unavailiable%,%10%\%CUR_DIR%\%Vert%,%10%\%CUR_DIR%\%Horz%,%10%\%CUR_DIR%\%Dgn1%,%10%\%CUR_DIR%\%Dgn2%,%10%\%CUR_DIR%\%move%,%10%\%CUR_DIR%\%alternate%,%10%\%CUR_DIR%\%link%"

[Scheme.Cur]
Arrow.cur
Help.cur
Working.ani
Busy.ani
Text.cur
Unavailable.cur
Vertical.cur
Horizontal.cur
Diag1.cur
Diag2.cur
Move.cur
Link.cur
Precision.cur
Handwriting.cur
Alternate.cur

[Scheme.Txt]

[Strings]
CUR_DIR       = "Cursors\Umbrella-xdr"
SCHEME_NAME   = "Umbrella, inc."
pointer       = "Arrow.cur"
help          = "Help.cur"
work          = "Working.ani"
busy          = "Busy.ani"
text          = "Text.cur"
unavailiable  = "Unavailable.cur"
vert          = "Vertical.cur"
horz          = "Horizontal.cur"
dgn1          = "Diag1.cur"
dgn2          = "Diag2.cur"
move          = "Move.cur"
link          = "Link.cur"
cross         = "Precision.cur"
hand          = "Handwriting.cur"
alternate     = "Alternate.cur"