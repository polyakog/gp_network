[Version]
signature="$CHICAGO$"

[DefaultInstall]
CopyFiles = Scheme.Cur, Scheme.Txt
AddReg    = Scheme.Reg

[DestinationDirs]
Scheme.Cur = 10,"%CUR_DIR%"
Scheme.Txt = 10,"%CUR_DIR%"

[Scheme.Reg]
HKCU,"Control Panel\Cursors\Schemes","%SCHEME_NAME%",,"%10%\%CUR_DIR%\%pointer%,%10%\%CUR_DIR%\%help%,%10%\%CUR_DIR%\%work%,%10%\%CUR_DIR%\%busy%,%10%\%CUR_DIR%\%cross%,%10%\%CUR_DIR%\%Text%,%10%\%CUR_DIR%\%Hand%,%10%\%CUR_DIR%\%unavailiable%,%10%\%CUR_DIR%\%Vert%,%10%\%CUR_DIR%\%Horz%,%10%\%CUR_DIR%\%Dgn1%,%10%\%CUR_DIR%\%Dgn2%,%10%\%CUR_DIR%\%move%,%10%\%CUR_DIR%\%alternate%,%10%\%CUR_DIR%\%link%"

; -- Installed files

[Scheme.Cur]
arrow.cur
beam.cur
busy.ani
cross.cur
ew.cur
help.cur
link.ani
move.cur
nesw.cur
ns.cur
nwse.cur
pen.cur
unavail.cur
up.cur
working.ani

[Strings]
CUR_DIR       = "Cursors\DeCody"
SCHEME_NAME   = "DeCody"
pointer             = "arrow.cur"
help                  = "help.cur"
work                 = "working.ani"
busy                 = "busy.ani"
text                  = "beam.cur"
unavailiable     = "unavail.cur"
vert                   = "ns.cur"
horz                   = "ew.cur"
dgn1                  = "nwse.cur"
dgn2                  = "nesw.cur"
move                 = "move.cur"
link                    = "link.ani" 
cross                 = "cross.cur"
hand                  = "pen.cur"
alternate           = "up.cur"
